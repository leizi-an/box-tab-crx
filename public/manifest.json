{
  "name": "Chrome插件V3",
  "version": "1.0",
  "description": "玩具",
  "manifest_version": 3,
  "background": {
    "service_worker": "service_worker.js"
  },
  "content_scripts": [],
  "permissions": ["storage", "declarativeContent", "bookmarks"],
  "host_permissions": [],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["/app.png"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_popup": "",
    "default_icon": {
      "16": "/app.png",
      "32": "/app.png",
      "48": "/app.png",
      "128": "/app.png"
    },
    "default_title": "干！"
  },
  "icons": {
    "16": "/app.png",
    "32": "/app.png",
    "48": "/app.png",
    "128": "/app.png"
  }
}
